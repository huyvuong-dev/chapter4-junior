<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="manage_note">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="label" xsi:type="string" translate="true">Custom Manage Calendar</item>
                <item name="dataScope" xsi:type="string"/>
                <item name="sortOrder" xsi:type="number">6</item>
                <item name="collapsible" xsi:type="boolean">true</item>
                <item name="opened" xsi:type="boolean">true</item>
            </item>
        </argument>
        <field name="note">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="label" xsi:type="string" translate="true">Custom Calendar</item>
                    <item name="formElement" xsi:type="string">date</item>
                    <item name="component" xsi:type="string">Magenest_Calendar/js/view/custom_calendar</item>
                    <item name="dataScope" xsi:type="string">custom.calendar</item>
                    <item name="sortOrder" xsi:type="number">1</item>
                    <item name="scopeLabel" xsi:type="string">[GLOBAL]</item>
                </item>
            </argument>
        </field>
    </fieldset>
</form>